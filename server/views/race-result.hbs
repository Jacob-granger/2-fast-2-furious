{{>header}}

<main class='result'>
  <div>
    <h2 class='heading-text'>{{name}}</h2>
    <h2 class='heading-text'>IS THE WINNER</h2>
  </div>
  {{#if image}}
    <img src='{{image}}' class='snail-img' alt='An image of {{name}}'>
  {{else}}
    <img src='/snail.png' class='snail-img' alt='A really fast snail'>
  {{/if}}
  <button class='btn'><a href='#'>Race Another?</a></button>
</main>